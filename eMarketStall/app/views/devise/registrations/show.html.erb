
<h3>My profile</h3>
<hr>
<br>
<h1><%= @user.nome+" "+@user.cognome %></h1>


<div class="row align-items-center">
  <div class="col-md-6">
    <div class="row">
      <div class="p-5">
        <% if @user.images.length==0 %>
          <img src="https://www.sognipedia.it/wp-content/uploads/2011/08/colore-nero-sognare-significato-interpretazione-numeri.png">
        <% else %>
          <%= image_tag(@user.thumbnail) %>
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-md-6 p-5">
      <div class="row">
        <div class="col-md-8">
         <h4>Email: <%= @user.email %></h4>
          <% if @user.roles_mask == 1 %>
          <h4>Current role registered: seller</h4>
          <% elsif @user.roles_mask == 2 %>
          <h4>Current role registered: buyer</h4>
          <% else %>
          <h4>Welcome back, boss</h4>
          <% end %>
        </div>
      </div>
      <br>
      <br>
      <br>
    <% if can? :see, Product %>
      <div class="row">
        <div class="col-md-8">
          <h4><%= link_to 'View how your products have been reviewed', user_reviews_path(:user_id => @user.id) %></h4>
        </div>
        <div class="col-md-4">
          <% products=Product.where("user_id = ?", @user.id) %>
          <% sum=0 %>
          <% count=0 %>
          <% avg=0 %>
          <% products.each do |p| %>
            <% reviews=Review.where("product_id = ?", p.id) %>
            <% reviews.each do |r| %>
              <% sum+=r.rating %>
              <% count+=1 %>
            <% end %>
          <% end %>
